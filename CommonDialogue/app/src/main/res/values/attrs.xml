<?xml version="1.0" encoding="utf-8"?>
<resources>



    <!-- 自定义滚动条 -->
    <declare-styleable name="adBannerRollView">
        <attr name="dotNormalBackground" format="reference|color"/>
        <attr name="dotFocusBackGround" format="reference|color"/>
        <attr name="dotLocation" format="enum">
            <enum name="center" value="0"/>
            <enum name="right" value="2"/>
            <enum name="left" value="1"/>
        </attr>
        <!-- 宽度比例 float -->
        <attr name="widthProportion" format="float"/>
        <!-- 高度比例 float -->
        <attr name="heightProportion" format="float"/>
    </declare-styleable>
</resources>